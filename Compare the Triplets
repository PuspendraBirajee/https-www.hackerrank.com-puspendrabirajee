# include<stdio.h>
int main()
{
    int a[3];
    int b[3];
    int c=0,count=0,i;
    for( i=0; i<3;i++)
    {
       scanf("%d",&a[i]); 
    }
    for( i=0; i<3;i++)
    {
       scanf("%d",&b[i]); 
    }
    for(i=0; i<3; i++)
    {
    if(a[i]>b[i])
    {
        count++;
    }
    else if(a[i]<b[i])
    {
        c++;
    }
    }
    printf("%d %d",count,c);
}
